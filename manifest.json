{
    "name": "demo",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "demo",
    "icons": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "128": "img/128.png"
    },

    "browser_action": {
        "default_icon": "img/128.png",
        "default_title": "demo",
        "default_popup": "dist/pages/popup.html"
    },

    "background": {
        // "persistent": true,
        // "scripts": [
        //     "jquery.js"
        // ]
    },

    "permissions": [
        "activeTab"
    ],

    "web_accessible_resources": [
        "img/16.png",
        "img/32.png",
        "img/48.png",
        "img/128.png",
        "img/128.png",
        "dist/static/inserted/index.js",
        "dist/static/inserted/index.css"
    ],

    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*",
                "file://*/*"
            ],
            "js": [
                "dist/static/pre-action/index.js",
                "dist/static/content/index.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "content_security_policy": "style-src 'self' 'unsafe-inline';script-src 'self' 'unsafe-eval'; object-src 'self';unsafe-inline"
}
